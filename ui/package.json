  "devDependencies": {
    "openapi-typescript-codegen": "0.9.3",
    "fork-ts-checker-webpack-plugin": "6.4.0"
  },
  "dependencies": {
    "@devexperts/remote-data-ts": "2.0.5",
    "@monaco-editor/react": "4.3.1",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.2",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "12.20.36",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "5.3.2",
    "axios": "0.24.0",
    "fp-ts": "2.11.5",
    "graphviz-react": "1.2.0",
    "lodash.throttle": "4.1.1",
    "monaco-editor": "^0.30.0",
    "monocle-ts": "2.3.11",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "4.4.4",
    "use-effect-reducer": "0.7.0",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typescript:watch": "tsc --noEmit --watch",
    "typegen": "rm -rf src/generated && openapi --input ../compiler/swagger.json --output ./src/generated --exportModels=true --exportCore=false --exportServices=false --useUnionTypes",
    "format": "prettier --write .",
    "prettier": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 60
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
