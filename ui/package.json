  },
  "dependencies": {
    "@devexperts/remote-data-ts": "2.0.5",
    "@monaco-editor/react": "4.4.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.3",
    "@types/lodash.throttle": "4.1.7",
    "@types/node": "18.11.9",
    "@types/react": "17.0.52",
    "@types/react-dom": "18.0.9",
    "@types/react-router": "5.1.19",
    "@types/react-router-dom": "5.3.3",
    "axios": "1.1.3",
    "fp-ts": "2.13.1",
    "lodash.throttle": "4.1.1",
    "monaco-editor": "0.34.1",
    "monocle-ts": "2.3.13",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "5.3.4",
    "react-scripts": "5.0.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3",
    "use-effect-reducer": "0.7.0",
    "web-vitals": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typescript:watch": "tsc --noEmit --watch",
    "typegen": "rm -rf src/generated && openapi --input ../compiler/swagger.json --output ./src/generated --exportModels=true --exportCore=false --exportServices=false --useUnionTypes",
    "format": "prettier --write .",
    "prettier": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 60
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
