{
  "name": "mimsa-ui",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "openapi-typescript-codegen": "0.9.3"
  },
  "dependencies": {
    "@devexperts/remote-data-ts": "2.0.5",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "11.1.1",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "26.0.24",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "12.20.36",
    "@types/react": "17.0.34",
    "@types/react-dom": "16.9.14",
    "@types/react-monaco-editor": "0.16.0",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "5.1.6",
    "axios": "0.24.0",
    "fp-ts": "2.10.5",
    "graphviz-react": "1.2.0",
    "lodash.throttle": "4.1.1",
    "monaco-editor-webpack-plugin": "5.0.0",
    "monocle-ts": "2.3.11",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-app-rewired": "2.1.8",
    "react-dom": "17.0.1",
    "react-monaco-editor": "0.46.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.4.4",
    "use-effect-reducer": "0.7.0",
    "web-vitals": "0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "typescript:watch": "tsc --noEmit --watch",
    "typegen": "rm -rf src/generated && openapi --input ../compiler/swagger.json --output ./src/generated --exportModels=true --exportCore=false --exportServices=false --useUnionTypes",
    "format": "prettier --write .",
    "prettier": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 60
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
