type Either e a = Left e | Right a

def orDefault (default: a) (value: Either e a): a {
  case value {
    Right a ->
       a,
    Left _ ->
       default
  }
}

def fmap (f: a -> b) (value: Either e a): Either e b {
  case value {
    Right a ->
       Right (f a),
    Left e ->
       Left e
  }
}
