\parserF ->
  \parserA ->
    \input ->
      match (parserF input) with 
          (Just (f, input2)) -> 
            match (parserA input2) with 
                (Just (a, input3)) -> 
                  (Just (((f a),
                          input3)))
              | _ -> 
                  Nothing
        | _ -> 
            Nothing