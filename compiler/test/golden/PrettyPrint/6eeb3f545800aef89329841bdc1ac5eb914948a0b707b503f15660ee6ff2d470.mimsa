\parserF ->
  \parserA ->
    let (Parser pF) =
      parserF;

    let (Parser pA) =
      parserA;

    (Parser (\input ->
      match (pF input) with 
          (Just (f, input2)) -> 
            (match (pA input2) with 
                (Just (a, input3)) -> 
                  (Just (((f a),
                          input3)))
              | _ -> 
                  Nothing)
        | _ -> 
            Nothing))