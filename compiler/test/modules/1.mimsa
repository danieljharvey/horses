def main: String = fromMaybe (id log) (Just (const (exclaim horse) False))

def id a = a

def const a b = a

def exclaim (str: String) = str ++ "!!!!!!!"

def horse = "fucking horse"

def fromMaybe (default: a) (maybeA: Maybe a): a = 
    match maybeA  with
      Just a -> a
      | _ -> default


def log = "Yes"

type Maybe a = Just a | Nothing


